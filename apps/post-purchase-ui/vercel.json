{
    "buildCommand": "npm run build",
    "outputDirectory": "build/client",
    "functions": {
        "api/index.js": {
            "memory": 1024,
            "maxDuration": 10
        }
    },
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/api/index"
        }
    ]
}